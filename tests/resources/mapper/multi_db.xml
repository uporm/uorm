<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//uporm.github.io//DTD Mapper 1//EN" "https://uporm.github.io/dtd/uorm-1-mapper.dtd">
<mapper namespace="multi_db">
    <select id="get_date" >
        SELECT 'default'
    </select>
    <select id="get_date"  databaseType="mysql">
        SELECT 'mysql'
    </select>
    <select id="get_date" databaseType="postgres">
        SELECT 'postgres'
    </select>
</mapper>